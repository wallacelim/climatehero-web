{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "csrfProtection": false,
      "destination": "climatehero-server"
    },
    {
      "source": "/(.*)",
      "localDir": "./build",
      "target": "$1"
    }
  ]
}